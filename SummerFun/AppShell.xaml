<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="SummerFun.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:SummerFun"
	xmlns:pages="clr-namespace:SummerFun.Pages"
    Shell.FlyoutBehavior="Flyout"
    Title="SummerFun">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate pages:MainPage}"
        Route="MainPage" />
	<ShellContent
		Title="Schedule"
		ContentTemplate="{DataTemplate pages:Schedule}"
		Route="Schedule" />
	<ShellContent
		Title="Diet"
		ContentTemplate="{DataTemplate pages:Diet}"
		Route="Diet" />
	<ShellContent
		Title="Exercises"
		ContentTemplate="{DataTemplate pages:Exercises}"
		Route="Exercises" />
	<ShellContent
		Title="Work Out"
		ContentTemplate="{DataTemplate pages:WorkOut}"
		Route="WorkOut" />
	<ShellContent
		Title="OverView"
		ContentTemplate="{DataTemplate pages:OverView}"
		Route="OverView" />
</Shell>
